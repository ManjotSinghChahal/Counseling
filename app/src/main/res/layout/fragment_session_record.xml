    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:id="@+id/cardview_booking_history"
        android:layout_marginRight="@dimen/_12sdp"
        android:layout_marginLeft="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_below="@+id/rel_spinner_detail"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_10sdp"
            android:paddingBottom="@dimen/_10sdp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="@dimen/_13sdp"
                android:text="Session 1"
                android:textColor="@color/colorBlack"
                android:layout_marginLeft="@dimen/_16sdp"/>


            <RelativeLayout
                android:layout_width="@dimen/_36sdp"
                android:layout_height="wrap_content"
                android:id="@+id/rel_videoBtn"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true">
                <ImageView
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/video_chat_speech_bubble_icon"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="@dimen/_24sdp"
                android:layout_height="wrap_content"
                android:id="@+id/rel_callAns"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/rel_videoBtn">
                <ImageView
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/call_answer_icon"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_centerVertical="true"
                android:paddingTop="@dimen/_2sdp"
                android:paddingBottom="@dimen/_2sdp"
                android:background="@drawable/roundedcorners_only_grey"
                android:layout_toLeftOf="@+id/rel_callAns">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Start"
                   android:paddingTop="@dimen/_1sdp"
                   android:paddingBottom="@dimen/_1sdp"
                   android:paddingLeft="@dimen/_14sdp"
                   android:paddingRight="@dimen/_14sdp"
                   android:textSize="@dimen/_11sdp"
                   android:layout_centerInParent="true"/>
            </RelativeLayout>


        </RelativeLayout>

    </android.support.v7.widget.CardView>


